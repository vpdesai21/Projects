package com.tcs.ilp.crud.utility;

/**
 * This is a custom Exception. This type of exception will be
 * thrown when we add a new news or event to the database. The method will
 * first check if the news or event is already event is already present in 
 * the database,else throws an exception.
 * 
 * @author sangeet(675067)
 * @version 1.0
 * @Creation 13/08/2012
 */
public class NewsAndEventsExistsException extends Exception

{

	private static final long serialVersionUID = 1L;

	public NewsAndEventsExistsException() 
	{
		
		
	}
	public String toString()
	{
		String msg = null;
		msg = "This news or event already exists ";
		
		return msg;
		
	}
	
}
