public class Amenity {
	
	/**
	 * @param amenityId
	 * @param amenityName
	 * @param amenityType
	 * @param amenityImage
	 * @param amenityDescription
	
	 */
	
	private String amenityId;
	private String amenityName;
	private AmenityType amenityType;
	private String amenityImage;
	private String amenityDescription;
	/*
	 * getter And Setter Methods for all private Attributes..
	 */
	/**
	 * @return the amenityId
	 */
	public String getAmenityId() {
		return amenityId;
	}
	/**
	 * @param AmenityId the AmenityId to set
	 */
	public void setAmenityId(String amenityId) {
		this.amenityId = amenityId;
	}
	/**
	 * @return the amenityName
	 */
	public String getAmenityName() {
		return amenityName;
	}
	/**
	 * @param amenityName the amenityName to set
	 */
	public void setAmenityName(String amenityName) {
		this.amenityName = amenityName;
	}
	/**
	 * @return the amenityImage
	 */
	public String getAmenityImage() {
		return amenityImage;
	}
	/**
	 * @param amenityImage the amenityImage to set
	 */
	public void setAmenityImage(String amenityImage) {
		this.amenityImage = amenityImage;
	}
	/**
	 * @return the amenityDescription
	 */
	public String getAmenityDescription() {
		return amenityDescription;
	}
	/**
	 * @param amenityDescription the amenityDescription to set
	 */
	public void setAmenityDescription(String amenityDescription) {
		this.amenityDescription = amenityDescription;
	}
	
	/**
	 * @param amenityType the amenityType to set
	 */
	public void setAmenityType(AmenityType amenityType) {
		this.amenityType = amenityType;
	}
	
	/**
	 * @return the amenityType
	 */
	public AmenityType getAmenityType() {
		return amenityType;
	}
}